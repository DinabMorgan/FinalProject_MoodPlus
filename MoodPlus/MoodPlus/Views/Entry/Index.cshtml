@model IEnumerable<MoodPlus.Models.Entry>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<div>
    <h4>Entry</h4>
    <hr />
    @foreach ( Entry entry in Model) {
        <h5>@entry.Body</h5>
        <div>
            <a asp-action="Update" asp-route-id="@entry?.Id">Update</a> |
            <a asp-action="Delete" asp-route-id="@entry?.Id">Delete</a> |
            <a asp-action="Create" asp-route-id="@entry?.Id">Create</a>
        </div>
    }
</div>
